{
	"name": "OpenCart DevContainer",
	"dockerComposeFile": [
		"docker-compose.yml"
	],
	"service": "web",
	"workspaceFolder": "/workspaces",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-php.php-pack",
				"ms-vscode.vscode-typescript-next",
				"dbaeumer.vscode-eslint"
			],
			"settings": {
				"php.validate.executablePath": "/usr/local/bin/php"
			}
		}
	},
	"forwardPorts": [
		8080
	],
	"features": {
		"docker-in-docker": "latest"
	}
}